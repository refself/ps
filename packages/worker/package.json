{
	"name": "@workflows/worker",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"cf-typegen": "wrangler types",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"db:generate:d1": "drizzle-kit generate --config=drizzle-d1.config.ts",
		"db:generate:do": "drizzle-kit generate --config=drizzle-do.config.ts",
		"db:generate:coordinator": "drizzle-kit generate --config=drizzle-connection-coordinator.config.ts",
		"db:generate": "pnpm db:generate:d1 && pnpm db:generate:do && pnpm db:generate:coordinator",
		"db:migrate:d1:local": "wrangler d1 migrations apply workflows --local"
	},
	"devDependencies": {
		"drizzle-kit": "^0.31.4",
		"typescript": "^5.5.2",
		"wrangler": "^4.38.0"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.36",
		"@hono/zod-validator": "^0.7.3",
		"@workflows/core": "workspace:*",
		"agents": "^0.2.0",
		"ai": "^5.0.37",
		"drizzle-orm": "^0.44.5",
		"hono": "^4.9.8",
		"nanoid": "^5.0.7",
		"workers-ai-provider": "2.0.0",
		"zod": "^4.1.11"
	}
}
